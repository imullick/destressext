{
"manifest_version": 2,
"name": "Stressbuster",
"description": "See cute pictures of dogs and cats and de-stress :)",
"version": "1.0",
"content_security_policy": "script-src 'self'; object-src 'self'",
"browser_action": {
"default_icon": "icon.png",
"default_popup": "window.html"
},
"permissions": ["storage"]
}
